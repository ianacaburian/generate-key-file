{
    "name": "@ianacaburian/generate-key-file",
    "version": "0.0.4",
    "description": "Provides juce_KeyGeneration::generateKeyFile() for javascript.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "open:juce-generate-key-file": "cd test/juce-generate-key-file && ./script/open-xcode-macos-dev.sh",
        "install:juce-generate-key-file": "cmake -S test/juce-generate-key-file -B test/juce-generate-key-file/build && cmake --build test/juce-generate-key-file/build && cmake --install test/juce-generate-key-file/build --prefix test/bin",
        "clean:juce-generate-key-file": "cd test/juce-generate-key-file && ./script/clean-build.sh",
        "test": "vitest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ianacaburian/generate-key-file.git"
    },
    "keywords": [
        "key-file",
        "juce",
        "rsa"
    ],
    "author": "ianacaburian",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ianacaburian/generate-key-file/issues"
    },
    "homepage": "https://github.com/ianacaburian/generate-key-file#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.0",
        "typescript": "^5.6.2",
        "vitest": "^2.1.1"
    },
    "dependencies": {
        "fast-xml-parser": "^4.5.0"
    }
}
